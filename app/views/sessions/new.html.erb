
<% provide(:title,'Login') %>
<div class = 'row'>
<div class='span12'>
	<h1>Login<h1>

	<%=  form_for(:session, url: sessions_path) do |f| %>
	<div class="field">
		<% f.label :email %>
		<% f.text_field :email %>
	</div>
	<div class="field">
		<% f.label :password %>
		<% f.password_field :password %>
	</div>
	<div class="actions">
		<%= f.submit "Login", class: 'btw btn-primary btn-large' %>
	</div>
<% end %>
</div>
</div>



